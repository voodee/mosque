/* Responsive */

@media only screen and (min-width: 768px)
  body
    &::before
      content 'desktop'

  .cd-section
    height 100vh
    &:first-of-type
      .cd-block
        visibility visible

  .cd-half-block
    height 100vh !important
    width 50%
    position absolute
    top 0
    &:nth-of-type(2)
      padding 6%

  .cd-block
    position fixed
    width 100%
    min-height 100vh
    top 0
    left 0
    height 100vh
    box-shadow 0 0 40px rgba(0, 0, 0, 0.7)
    visibility hidden
    & > *
      visibility visible

  .cd-vertical-nav
    display block

  .cd-section:nth-of-type(even) .cd-half-block:first-of-type, .cd-section:nth-of-type(odd) .cd-half-block:nth-of-type(2)
    left 0
    transform translateX(-100%)

  .cd-section:nth-of-type(odd) .cd-half-block:first-of-type, .cd-section:nth-of-type(even) .cd-half-block:nth-of-type(2)
    right 0
    transform translateX(100%)



